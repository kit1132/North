{
  "manifest_version": 3,
  "name": "Tab Groups Cleaner",
  "version": "1.0.0",
  "description": "タブグループを一括削除・管理できる拡張機能。Chromeのメモリを節約します。",
  "permissions": [
    "tabGroups",
    "tabs",
    "storage",
    "sessions"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
